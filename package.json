{
  "name": "gin-mvc",
  "version": "1.0.0",
  "description": "Building assets with esbuild",
  "scripts": {
    "build:js": "node ./.esbuild.config.js",
    "build:css": "tailwindcss -i ./web/public/stylesheets/tailwind.css -o ./build/web/public/stylesheets/tailwind.css --minify",
    "build": "run-s build:js build:css",
    "dev:js": "node ./.esbuild.config.watch.js",
    "dev:css": "tailwindcss -i ./web/public/stylesheets/tailwind.css -o ./build/web/public/stylesheets/tailwind.css --watch",
    "dev": "run-p dev:js dev:css",
    "format": "prettier --config ./.prettierrc.js --write .",
    "format:check": "prettier --config ./.prettierrc.js --check .",
    "lint": "eslint --config ./.eslintrc.js \"**/*.{js,ts}\" --fix",
    "lint:check": "eslint --config ./.eslintrc.js \"**/*.{js,ts}\"",
    "postinstall": "node ./.postinstall.js",
    "test": "jest --config ./jest.config.js --detectOpenHandles --forceExit --logHeapUsage",
    "test:cov": "jest --config ./jest.config.js --coverage --detectOpenHandles --forceExit --logHeapUsage"
  },
  "keywords": [
    "golang",
    "gin",
    "mvc"
  ],
  "author": "heitorgcarneiro@gmail.com",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.2.5",
    "@types/jsdom": "20.0.1",
    "@types/node": "18.11.12",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "autoprefixer": "10.4.13",
    "daisyui": "2.46.1",
    "esbuild": "0.16.12",
    "esbuild-jest": "0.5.0",
    "esbuild-plugin-manifest": "0.5.0",
    "eslint": "8.31.0",
    "jest": "29.3.1",
    "jsdom": "20.0.3",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.20",
    "prettier": "2.8.1",
    "prettier-plugin-tailwindcss": "0.2.1",
    "shelljs": "0.8.5",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4",
    "vue": "3.2.45"
  },
  "dependencies": {
    "axios": "1.2.2"
  }
}
